@prefix : <http://elevator.com/ontology#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# 核心类
:WallPanel a rdfs:Class ;
    rdfs:label "壁板" ;
    rdfs:comment "电梯轿厢壁板组件" .

:Joint a rdfs:Class ;
    rdfs:label "配合关系" ;
    rdfs:comment "壁板之间的配合关系" .

:DimensionCalculation a rdfs:Class ;
    rdfs:label "尺寸计算" ;
    rdfs:comment "壁板尺寸计算问题类型" .

:CostCalculation a rdfs:Class ;
    rdfs:label "成本计算" ;
    rdfs:comment "壁板成本计算问题类型" .

:DesignChange a rdfs:Class ;
    rdfs:label "设计变更" ;
    rdfs:comment "设计变更处理问题类型" .

# 属性定义
:hasFace a rdf:Property ;
    rdfs:domain :WallPanel ;
    rdfs:range :Face ;
    rdfs:label "具有面" .

:hasJoint a rdf:Property ;
    rdfs:domain :WallPanel ;
    rdfs:range :Joint ;
    rdfs:label "具有配合关系" .

:jointType a rdf:Property ;
    rdfs:domain :Joint ;
    rdfs:range :JointType ;
    rdfs:label "配合类型" .

:part1 a rdf:Property ;
    rdfs:domain :Joint ;
    rdfs:range :WallPanel ;
    rdfs:label "配合对象1" .

:part2 a rdf:Property ;
    rdfs:domain :Joint ;
    rdfs:range :WallPanel ;
    rdfs:label "配合对象2" .

:faceSelectionMethod a rdf:Property ;
    rdfs:domain :Face ;
    rdfs:range :SelectionMethod ;
    rdfs:label "面选择方法" .

# 枚举类型
:JointType a rdfs:Class ;
    rdfs:label "配合类型" .

:Contact a :JointType ;
    rdfs:label "接触" .

:Align a :JointType ;
    rdfs:label "平齐" .

:Face a rdfs:Class ;
    rdfs:label "壁板面" .

:InnerFace a :Face ;
    rdfs:label "内面" .

:OuterFace a :Face ;
    rdfs:label "外面" .

:TopFace a :Face ;
    rdfs:label "顶面" .

:BottomFace a :Face ;
    rdfs:label "底面" .

:LeftFace a :Face ;
    rdfs:label "左面" .

:RightFace a :Face ;
    rdfs:label "右面" .

:SelectionMethod a rdfs:Class ;
    rdfs:label "面选择方法" .

:ParallXZNearest a :SelectionMethod ;
    rdfs:label "parall-xz-nearest-face" .

:ParallFarthestXZ a :SelectionMethod ;
    rdfs:label "parall-farthest_xz_face" .

:ParallFarthestXY a :SelectionMethod ;
    rdfs:label "parall-farthest-xy-face" .

:ParallXYNearest a :SelectionMethod ;
    rdfs:label "parall-xy-nearest-face" .

:ParallYZNearest a :SelectionMethod ;
    rdfs:label "parall-yz-nearest-face" .

:ParallFarthestYZ a :SelectionMethod ;
    rdfs:label "parall-farthest_yz_face" .

# 尺寸属性
:height a rdf:Property ;
    rdfs:domain :WallPanel ;
    rdfs:range xsd:float ;
    rdfs:label "高度" .

:width a rdf:Property ;
    rdfs:domain :WallPanel ;
    rdfs:range xsd:float ;
    rdfs:label "宽度" .

:thickness a rdf:Property ;
    rdfs:domain :WallPanel ;
    rdfs:range xsd:float ;
    rdfs:label "厚度" .

:bendHeight a rdf:Property ;
    rdfs:domain :WallPanel ;
    rdfs:range xsd:float ;
    rdfs:label "折弯高度" .

:boltHoleCount a rdf:Property ;
    rdfs:domain :WallPanel ;
    rdfs:range xsd:integer ;
    rdfs:label "螺孔数量" .

:reinforcementCount a rdf:Property ;
    rdfs:domain :WallPanel ;
    rdfs:range xsd:integer ;
    rdfs:label "加强筋数量" .

# 成本属性
:materialCost a rdf:Property ;
    rdfs:domain :WallPanel ;
    rdfs:range xsd:float ;
    rdfs:label "材料成本" .

:processingCost a rdf:Property ;
    rdfs:domain :WallPanel ;
    rdfs:range xsd:float ;
    rdfs:label "加工成本" .

:totalCost a rdf:Property ;
    rdfs:domain :WallPanel ;
    rdfs:range xsd:float ;
    rdfs:label "总成本" .
